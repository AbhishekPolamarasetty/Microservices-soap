<!-- This schema contains data types used to describe Tariffs -->

<xs:schema targetNamespace="http://mdsuk.com/ws/dise3g/agreementtariff/dto/business"
    xmlns:target="http://mdsuk.com/ws/dise3g/agreementtariff/dto/business"
    xmlns:base="http://mdsuk.com/ws/dise3g/base" xmlns:xs="http://www.w3.org/2001/XMLSchema">

    <xs:import namespace="http://mdsuk.com/ws/dise3g/base" schemaLocation="base.xsd"/>

    <xs:complexType name="AgreementTariffType">
        <xs:all>
            <xs:element name="Company" type="base:CompanyCodeType" minOccurs="1"/>
            <xs:element name="ATEffectiveDate" type="xs:date" minOccurs="1"/>
            <xs:element name="TariffCode" type="target:TariffCodeType" minOccurs="1"/>
        </xs:all>
    </xs:complexType>

    <xs:simpleType name="TariffCodeType">
        <xs:restriction base="base:StringMaxLength6NotRequiredType"/>
    </xs:simpleType>

</xs:schema>
