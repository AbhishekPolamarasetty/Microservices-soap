<!-- This schema describes the API data types for the OAM service -->

<xs:schema targetNamespace="http://mdsuk.com/ws/dise3g/oam/dto/service"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:oam="http://mdsuk.com/ws/dise3g/oam/dto/business"
    xmlns:base="http://mdsuk.com/ws/dise3g/base">

    <xs:import namespace="http://mdsuk.com/ws/dise3g/oam/dto/business" schemaLocation="oam.xsd"/>
    <xs:import namespace="http://mdsuk.com/ws/dise3g/base" schemaLocation="base.xsd"/>

    <xs:complexType name="ProvisionOAMAccessType">
        <xs:all>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/>
            <xs:element name="OAMData" type="oam:ProvisionOAMType" minOccurs="1"/>
        </xs:all>
    </xs:complexType>


    <xs:complexType name="ProvisionOAMAccessResponseType">
        <xs:all>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/>
        </xs:all>
    </xs:complexType>
</xs:schema>
