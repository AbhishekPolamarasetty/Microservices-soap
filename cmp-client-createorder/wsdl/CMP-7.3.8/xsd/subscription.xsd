<!-- This schema describes data types used by the Subscription service. -->
<xs:schema targetNamespace="http://mdsuk.com/ws/dise3g/subscription/dto/business"
    xmlns:target="http://mdsuk.com/ws/dise3g/subscription/dto/business"
    xmlns:address="http://mdsuk.com/ws/dise3g/address/dto/business"
    xmlns:base="http://mdsuk.com/ws/dise3g/base"
    xmlns:order="http://mdsuk.com/ws/dise3g/order/dto/business"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:services="http://mdsuk.com/ws/dise3g/services/dto/business"
	xmlns:billing="http://mdsuk.com/ws/dise3g/billing/dto/business"
	xmlns:porting="http://mdsuk.com/ws/dise3g/porting/dto/business"
    xmlns:recordpayment="http://mdsuk.com/ws/dise3g/recordpayment/dto/business">


    <xs:import namespace="http://mdsuk.com/ws/dise3g/base" schemaLocation="base.xsd"/>
    <xs:import namespace="http://mdsuk.com/ws/dise3g/address/dto/business"
        schemaLocation="address.xsd"/>
    <xs:import namespace="http://mdsuk.com/ws/dise3g/order/dto/business"
        schemaLocation="orderprocessing.xsd"/>
    <xs:import namespace="http://mdsuk.com/ws/dise3g/services/dto/business"
    	schemaLocation="services.xsd"/>
    <xs:import namespace="http://mdsuk.com/ws/dise3g/billing/dto/business"
    	schemaLocation="billing.xsd"/> 
    <xs:import namespace="http://mdsuk.com/ws/dise3g/porting/dto/business"
    	schemaLocation="porting.xsd"/> 
    <xs:import namespace="http://mdsuk.com/ws/dise3g/recordpayment/dto/business"
        schemaLocation="recordpayment.xsd"/>


    <xs:complexType name="SubscriptionConnectionType">
        <xs:all>
            <xs:element name="AgreementNumber" type="base:AgreementNumberType" minOccurs="0"/>
            <xs:element name="EnquirySource" type="base:EnquirySourceCodeType" minOccurs="0"/>
            <xs:element name="CampaignNumber" type="base:CampaignCodeType" minOccurs="0"/>
            <xs:element name="PricePlan" type="base:PricePlanCodeType" minOccurs="0"/>
            <xs:element name="TariffCode" type="target:TariffCodeType" minOccurs="0"/>
            <xs:element name="PackageCode" type="target:PackageCodeType" minOccurs="0"/>
            <xs:element name="IgnoreDefaultPackage" type="xs:boolean" minOccurs="0"/>
            <xs:element name="SalesAccountCode" type="base:AccountNumberType" minOccurs="0"/>
            <xs:element name="UserName" type="target:UserNameType" minOccurs="0"/>
            <xs:element name="CustomerCostCentre" type="target:CustomerCostCentreType" minOccurs="0"/>
            <xs:element name="CustomerReference" type="target:CustomerReferenceCodeType" minOccurs="0"/>
			<xs:element name="SubPassword" type="target:SubPasswordType" minOccurs="0"/>
            <xs:element name="DirectoryListingAllowed" type="xs:boolean" minOccurs="0"/>
            <xs:element name="ManagedSerialNumbers" type="target:ManagedSerialNumbersType"
                minOccurs="0"/>
            <xs:element name="NonManagedSerialNumbers" type="target:NonManagedSerialNumbersType"
                minOccurs="0"/>
            <xs:element name="NetworkSubCodes" type="target:NetworkSubCodesType" minOccurs="0"/>
            <xs:element name="APNList" type="target:APNListType" minOccurs="0"/>
            <xs:element name="ConnectionDetails" type="target:ConnectionDetailsType" minOccurs="1"/>
            <xs:element name="Address" type="address:AddressType" minOccurs="0"/>
            <xs:element name="Contract" type="target:ContractType" minOccurs="0"/>
            <xs:element name="EmailData" type="target:CreateEmailType" minOccurs="0"/>
            <xs:element name="AttributeGroup" type="billing:AttributeGroupType" minOccurs="0"/>
            <xs:element name="DonorLedPortDetails" type="target:SubscriptionDonorLedPortDetailsType" minOccurs="0"/>
			<xs:element name="PostPayCaps" type="target:PostPayCapsType" minOccurs="0" maxOccurs="1"/>
			<xs:element name="AccountBond" type="target:RecordAccountBondType" minOccurs="0" maxOccurs="1"/>
			<xs:element name="ParentSubscription" type="xs:boolean" minOccurs="0" />
			<xs:element name="BoltOnPackages" type="target:BoltOnPackageType" minOccurs="0"/>
			<xs:element name="CUGNumbers" type="target:CUGNumbersType" minOccurs="0"/>
        </xs:all>
    </xs:complexType>       

    <xs:complexType name="SubscriptionBasicType">
        <xs:annotation>
            <xs:documentation>This structure is returned as part of the Subscription Query response
                when BASIC dataset is specified</xs:documentation>
        </xs:annotation>
        <xs:all>
            <xs:element name="SubscriptionNumber" type="base:SubscriptionNumberType" minOccurs="1"/>
            <xs:element name="AgreementNumber" type="base:AgreementNumberType" minOccurs="1"/>
            <xs:element name="AccountNumber" type="base:AccountNumberType" minOccurs="1"/>
            <xs:element name="CorporateCode" type="xs:string" minOccurs="0"/>
            <xs:element name="GroupCode" type="xs:string" minOccurs="0"/>
            <xs:element name="CompanyNumber" type="base:CompanyNumberType" minOccurs="1"/>
            <xs:element name="SalesAccountCode" type="base:SalesAccountCodeType" minOccurs="0"/>
            <xs:element name="SubscriptionStatus" type="target:SubscriptionStatusType"  minOccurs="1"/>
			<xs:element name="LastSubscriptionOrderStatus" type="target:LastOrderStatusType" minOccurs="0"/>
            <xs:element name="PricePlanCode" type="base:PricePlanCodeType" minOccurs="0"/>
            <xs:element name="TariffChangePending" type="xs:boolean" minOccurs="1" />
            <xs:element name="TariffCode" type="target:TariffCodeType" minOccurs="1"/>
            <xs:element name="TariffShareGroup" type="target:TariffShareGroupType" minOccurs="0"/>
            <xs:element name="BillingType" type="target:BillingTypeType" minOccurs="1"/>
            <xs:element name="NetworkCode" type="base:NetworkCodeType" minOccurs="1"/>
            <xs:element name="ConnectedDate" type="xs:date" minOccurs="0"/>
            <xs:element name="DisconnectedDate" type="xs:date" minOccurs="0"/>
            <xs:element name="TerminateOn" type="xs:date" minOccurs="0"/>
            <xs:element name="ConnectionReason" type="target:ConnectionReasonType" minOccurs="1"/>
            <xs:element name="AddressNumber" type="address:AddressCodeType" minOccurs="1"/>
            <xs:element name="EmailAddress" type="xs:string" minOccurs="0"/>
			<xs:element name="Parent" type="xs:boolean" minOccurs="0" />
			<xs:element name="LastTariffChangeEvent" type="base:NineDigitNumberType" minOccurs="0"/> 
			<xs:element name="LastNumberChangeEvent" type="base:NineDigitNumberType" minOccurs="0"/>
			<xs:element name="LastConnectionEvent" type="base:NineDigitNumberType" minOccurs="0"/>
			<xs:element name="SuspendedFromInvoicing" type="base:SuspendedFromInvoicingType" minOccurs="0"/>
            <xs:element name="LastAmendedDate" type="base:LastAmendedDateType" minOccurs="0"/>
        </xs:all>
    </xs:complexType>

    <xs:complexType name="CustomerDetailsType">
        <xs:annotation>
            <xs:documentation>This structure is returned as part of the Subscription Query response
                when CUSTOMERDETAILS dataset is specified</xs:documentation>
        </xs:annotation>
        <xs:all>
            <xs:element name="UserName" type="xs:string" minOccurs="0"/>
            <xs:element name="SubPassword" type="xs:string" minOccurs="0"/>
            <xs:element name="ItemCode" type="xs:string" minOccurs="0"/>
            <xs:element name="DirectoryListingAllowed" type="xs:boolean" minOccurs="0"/>
            <xs:element name="CustomerCostCentre" type="xs:string" minOccurs="0"/>
            <xs:element name="CustomerReference" type="xs:string" minOccurs="0"/>
            <xs:element name="LastAmendedDate" type="base:LastAmendedDateType" minOccurs="0"/>
        </xs:all>
    </xs:complexType>

    <xs:complexType name="ContractAndSalesType">
        <xs:annotation>
            <xs:documentation>This structure is returned as part of the Subscription Query response
                when CONTRACTANDSALES dataset is specified</xs:documentation>
        </xs:annotation>
        <xs:all>
            <xs:element name="ContractNumber" type="xs:string" minOccurs="0"/>
            <xs:element name="PricePlanCode" type="xs:string" minOccurs="0"/>
            <xs:element name="FirstInvoiceDate" type="xs:date" minOccurs="0"/>
            <xs:element name="LastInvoiceDate" type="xs:date" minOccurs="0"/>
            <xs:element name="NextInvoiceDate" type="xs:date" minOccurs="0"/>
            <xs:element name="ContractStartDate" type="xs:date" minOccurs="0"/>
            <xs:element name="ContractEndDate" type="xs:date" minOccurs="0"/>
			<xs:element name="MinimumContractEndDate" type="xs:date" minOccurs="0"/>
            <xs:element name="TermOfContract" type="base:ThreeDigitNumberType" minOccurs="0"/>
            <xs:element name="CarriedOverContractDays" type="base:ThreeDigitNumberType" minOccurs="0"/>
            <xs:element name="FreezeContractDays" type="base:ThreeDigitNumberType" minOccurs="0"/>
            <xs:element name="TermsAndConditionsGroupCode"
                type="xs:string" minOccurs="0"/>
            <xs:element name="TerminationInvoiceProduced" type="xs:boolean" minOccurs="0"/>
            <xs:element name="LastAmendedDate" type="base:LastAmendedDateType" minOccurs="0"/>
        </xs:all>
    </xs:complexType>

    <xs:complexType name="SerialNumbersType">
        <xs:annotation>
            <xs:documentation>This structure is returned as part of the Subscription Query response
                when SERIALNUMBERS dataset is specified</xs:documentation>
        </xs:annotation>
        <xs:all>
            <xs:element name="SerialNumbers" type="target:ManagedSerialNumbersResponseType" minOccurs="0"/>
            <xs:element name="NonManagedSerialNumbers" type="target:NonManagedSerialNumbersResponseType"
                minOccurs="0"/>
            <xs:element name="NetworkSubCodes" type="target:NetworkSubCodesResponseType" minOccurs="0"/>
        </xs:all>
    </xs:complexType>

    <xs:complexType name="BillingMediaType">
        <xs:annotation>
            <xs:documentation>This structure is returned as part of the Subscription Query response
                when BILLINGMEDIA dataset is specified</xs:documentation>
        </xs:annotation>
        <xs:all>
            <xs:element name="BillingMediaCode" type="xs:string" minOccurs="0"/>
            <xs:element name="BillingMediaCopyInvoiceMediaCode" type="xs:string"
                minOccurs="0"/>
            <xs:element name="BillingMediaOverrideInvoiceMediaCode" type="xs:string"
                minOccurs="0"/>
            <xs:element name="LastAmendedDate" type="base:LastAmendedDateType" minOccurs="0"/>
        </xs:all>
    </xs:complexType>

    <xs:complexType name="UsageAndCreditControlType">
        <xs:annotation>
            <xs:documentation>This structure is returned as part of the Subscription Query response
                when USAGEANDCREDITCONTROL dataset is specified</xs:documentation>
        </xs:annotation>
        <xs:all>
            <xs:element name="LowerUsageLimit" type="base:Currency9_2NotRequiredType" minOccurs="0"/>
            <xs:element name="UpperUsageLimit" type="base:Currency9_2NotRequiredType" minOccurs="0"/>
            <xs:element name="IgnoreUsageLimits" type="xs:boolean" minOccurs="0"/>
            <xs:element name="AutoAdjust" type="xs:boolean" minOccurs="0"/>
            <xs:element name="IgnoreCreditControlProcedures" type="xs:boolean" minOccurs="0"/>
            <xs:element name="LastAmendedDate" type="base:LastAmendedDateType" minOccurs="0"/>
        </xs:all>
    </xs:complexType>


    <xs:complexType name="UpdateSubscriptionBasicType">
        <xs:annotation>
            <xs:documentation>Update the Basic data associated with a Subscription</xs:documentation>
        </xs:annotation>
        <xs:all>
            <xs:element name="SalesAccountCode" type="base:AccountNumberType" minOccurs="0"/>
            <xs:element name="AddressNumber" type="address:AddressCodeType" minOccurs="0"/>
			<xs:element name="AgreementNumber" type="base:AgreementNumberType" minOccurs="0"/>
            <xs:element name="LastAmendedDate" type="base:LastAmendedDateType"/>
            <xs:element name="ParentSubscription" type="xs:boolean" minOccurs="0"/>
        </xs:all>
    </xs:complexType>

    <xs:complexType name="UpdateCustomerDetailsType">
        <xs:annotation>
            <xs:documentation>Update the Customer Details data associated with a
            Subscription</xs:documentation>
        </xs:annotation>
        <xs:all>
            <xs:element name="UserName" type="target:UserNameType" minOccurs="0"/>
            <xs:element name="SubPassword" type="target:SubPasswordType" minOccurs="0"/>
            <xs:element name="ItemCode" type="order:ItemCodeType" minOccurs="0"/>
            <xs:element name="DirectoryListingAllowed" type="xs:boolean" minOccurs="0"/>
            <xs:element name="CustomerCostCentre" type="target:CustomerCostCentreType"
                minOccurs="0"/>
                
            <xs:element name="CustomerReference" type="target:CustomerReferenceCodeType" minOccurs="0"/>
                
            <xs:element name="LastAmendedDate" type="base:LastAmendedDateType"/>
        </xs:all>
    </xs:complexType>


    <xs:complexType name="UpdateContractAndSalesType">
        <xs:annotation>
            <xs:documentation>Update the Contract And Sales data associated with a Subscription - TB
                check these are stored on Sub file rather than agreement file!!!</xs:documentation>
        </xs:annotation>
        <xs:all>
            <xs:element name="ContractNumber" type="target:ContractNumberType" minOccurs="0"/>
            <xs:element name="ContractStartDate" type="base:ClearableDateType"
                minOccurs="0"/>
            <xs:element name="TermOfContract" type="target:ContractTermType" minOccurs="0"/>
			<xs:element name="CarriedOverContractDays" type="base:ThreeDigitNumberType" minOccurs="0"/>
            <xs:element name="TermsAndConditionsGroupCode"
                type="target:TermsAndConditionsGroupCodeType" minOccurs="0"/>
			<xs:element name="LastAmendedDate" type="base:LastAmendedDateType"/>
        </xs:all>
    </xs:complexType>

    <xs:complexType name="UpdateUsageAndCreditControlType">
        <xs:annotation>
            <xs:documentation>Update the Usage And Credit Control data associated with a
                Subscription</xs:documentation>
        </xs:annotation>
        <xs:all>
            <xs:element name="LowerUsageLimit" type="base:Currency9_2Type" minOccurs="0"/>
            <xs:element name="UpperUsageLimit" type="base:Currency9_2Type" minOccurs="0"/>
            <xs:element name="IgnoreUsageLimits" type="xs:boolean" minOccurs="0"/>
            <xs:element name="AutoAdjust" type="xs:boolean" minOccurs="0"/>
            <xs:element name="IgnoreCreditControlProcedures" type="xs:boolean" minOccurs="0"/>
            <xs:element name="LastAmendedDate" type="base:LastAmendedDateType"/>
        </xs:all>
    </xs:complexType>

    <xs:complexType name="UpdateBillingMediaType">
        <xs:annotation>
            <xs:documentation>Update the Billing Media data associated with a
            Subscription</xs:documentation>
        </xs:annotation>
        <xs:all>
            <xs:element name="BillingMediaCode" type="target:MediaCategoryCodeType"
                minOccurs="0"/>
            <xs:element name="BilllingMediaCopyInvoiceMediaCode"
                type="target:MediaCategoryCodeType" minOccurs="0"/>
            <xs:element name="BillingMediaOverrideInvoiceMediaCode"
                type="target:MediaCategoryCodeType" minOccurs="0"/>
            <xs:element name="LastAmendedDate" type="base:LastAmendedDateType"/>
        </xs:all>
    </xs:complexType>

    <xs:complexType name="DiscountsOverridesType">
        <xs:annotation>
            <xs:documentation>This structure is returned as part of the Subscription Query response
                when DISCOUNTOVERRIDES dataset is specified</xs:documentation>
        </xs:annotation>
        <xs:all>
            <xs:element name="IgnoreAgreementDiscounts" type="xs:boolean" minOccurs="0"/>
            <xs:element name="IgnoreAccountDiscounts" type="xs:boolean" minOccurs="0"/>
            <xs:element name="LastAmendedDate" type="base:LastAmendedDateType" minOccurs="0"/>
        </xs:all>
    </xs:complexType>

    <xs:simpleType name="TariffCodeType">
        <xs:restriction base="base:StringMaxLength6Type"/>
    </xs:simpleType>
	
	<xs:simpleType name="LastOrderStatusType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="AWAITING_ORDER_DESPATCH"/>
			<xs:enumeration value="AWAITING_CUSTOMER_ACTIVATION"/>
			<xs:enumeration value="COMPLETE"/>
			<xs:enumeration value="CANCELLED"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="TariffShareGroupType">
		<xs:restriction base="base:StringMaxLength19Type"/>
    </xs:simpleType>

	<xs:simpleType name="BillingTypeType">
		<xs:restriction base="base:StringMaxLength8Type"/>
    </xs:simpleType>

    <xs:simpleType name="SubscriptionStatusType">
        <xs:restriction base="xs:string"/>
    </xs:simpleType>

    <xs:simpleType name="ContractTermType">
        <xs:restriction base="base:ThreeDigitNumber_NotZeroType"/>
    </xs:simpleType>
	
	<xs:simpleType name="SalesAccountIdentifierType">
    	<xs:restriction base="base:EightDigitNumberType"/>
    </xs:simpleType>

    <xs:complexType name="SubscriptionServicesType">
        <xs:annotation>
            <xs:documentation>This structure is returned as part of the some Query responses
                when SERVICES dataset is specified</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="SubscriptionService" type="target:SubscriptionServiceType"
                minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="SubscriptionServiceType">       
        <xs:all>
            <xs:element name="ServiceCode" type="target:ServiceCodeType" minOccurs="1"/>
            <xs:element name="EffectiveDate" type="xs:date" minOccurs="1"/>
            <xs:element name="ExpiryDate" type="xs:date" minOccurs="0"/>
            <xs:element name="LastAmendedDate" type="base:LastAmendedDateType" minOccurs="0"/>
        </xs:all>
    </xs:complexType>
	
	<xs:complexType name="SubscriptionServicesBasicType">
	<xs:sequence>
		<xs:element name="SubscriptionService" type="target:SubscriptionServiceBasicType"
			minOccurs="1" maxOccurs="unbounded"/>
	</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="SubscriptionServiceBasicType">
		<xs:complexContent>
			<xs:extension base="services:GenericServiceBasicType">
				<xs:sequence>
					<xs:element name="PackageCode" type="target:PackageCodeType" minOccurs="0"/>
					<xs:element name="PricePeriodCoverage" type="target:PricingPeriodCoverageType" minOccurs="0"/>
					<xs:element name="SalesAccountNumber" type="target:SalesPersonNumberType" minOccurs="0"/>
					<xs:element name="BillingOffset" type="target:BillingOffsetType" minOccurs="0"/>
					<xs:element name="AddingEvent" type="base:NineDigitNumberType" minOccurs="0"/>
					<xs:element name="NetworkConfirmed" type="xs:boolean" minOccurs="0"/>
					<xs:element name="Migrated" type="xs:boolean" minOccurs="0"/>
					<xs:element name="PerBill" type="xs:boolean" minOccurs="0"/>
					<xs:element name="BilledUntil" type="xs:date" minOccurs="0"/>
					<xs:element name="ServiceBarred" type="xs:boolean" minOccurs="0"/>
					<xs:element name="RecurringCharge" type="xs:boolean" minOccurs="0"/>
					<xs:element name="ContractService" type="xs:boolean" minOccurs="0"/>
					<xs:element name="ContractDuration" type="base:ThreeDigitNumberType" minOccurs="0"/>
					<xs:element name="ContractReference" type="services:ContractReferenceType" minOccurs="0"/>
					<xs:element name="SuppressRemainingCharge" type="xs:boolean" minOccurs="0"/>
					<xs:element name="PriceConsolidationService" type="target:ServiceCodeType" minOccurs="0"/>
					<xs:element name="LastAmendedDate" type="base:LastAmendedDateType" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>	
		</xs:complexContent>
	</xs:complexType>
	
	<xs:complexType name="PricingPeriodCoverageType">
        <xs:all>
            <xs:element name="CoverageType" type="target:CoverageTypeTypes" minOccurs="1"/>
            <xs:element name="CoverageValue" type="base:ThreeDigitNumberType" minOccurs="1"/>
        </xs:all>
    
	</xs:complexType>
		<xs:simpleType name="CoverageTypeTypes">
		<xs:restriction base="xs:string">
			<xs:enumeration value="DAYS"/>
			<xs:enumeration value="MONTHS"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="BillingOffsetType">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0"></xs:minInclusive>
			<xs:maxInclusive value="99"></xs:maxInclusive>
		</xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="ServiceCodeType">
        <xs:restriction base="base:StringMaxLength6Type"/>
    </xs:simpleType>

    <xs:complexType name="SubscriptionBarsType">
        <xs:sequence>
            <xs:element name="SubscriptionBar" type="target:SubscriptionBarType" minOccurs="1"
                maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="SubscriptionBarType">
        <xs:all>
            <xs:element name="NetworkCommandCode" type="xs:string" minOccurs="1"/>
            <xs:element name="BarredDate" type="xs:dateTime" minOccurs="1"/>
            <xs:element name="UnBarredDate" type="xs:dateTime" minOccurs="0"/>
            <xs:element name="BarReasonCode" type="xs:string" minOccurs="0"/>
            <xs:element name="UnbarReasonCode" type="xs:string" minOccurs="0"/>
            <xs:element name="AdminBarFlag" type="xs:boolean" minOccurs="0"/>
            <xs:element name="LastAmendedDate" type="base:LastAmendedDateType" minOccurs="0"/>
        </xs:all>
    </xs:complexType>

    <xs:complexType name="QueryPostPayCapsType">
        <xs:annotation>
            <xs:documentation>This structure is returned as part of the Subscription Query response
                when POSTPAYCAPS dataset is specified</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Cap" type="target:QueryPostPayCapType"
                minOccurs="1" maxOccurs="999"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="QueryPostPayCapType">
        <xs:all>
            <xs:element name="CapCode" type="target:CapCodeType" minOccurs="0"/>
            <xs:element name="Description" type="target:CapDescriptionType" minOccurs="0"/>
			<xs:element name="EffectiveDate" type="xs:date" minOccurs="0"/>
			<xs:element name="ExpiryDate" type="xs:date" minOccurs="0"/>
            <xs:element name="Amount" type="target:CapValueType" minOccurs="0"/>
 			<xs:element name="UnitOfMeasure" type="base:StringMaxLength8NotRequiredType" minOccurs="0"/>
            <xs:element name="CapType" type="target:CapTypeType" minOccurs="0"/>
            <xs:element name="CapConsumption" type="target:CapValueType" minOccurs="0"/>
            <xs:element name="CapDataType" type="target:CapDataTypeType" minOccurs="0"/>
            <xs:element name="ThresholdWorkflow" type="base:NineDigitNumberType" minOccurs="0"/>
            <xs:element name="BreachWorkflow" type="base:NineDigitNumberType" minOccurs="0"/>
            <xs:element name="DefaultCapAmount" type="target:CapValueType" minOccurs="0"/>
            <xs:element name="ResetAmount" type="target:CapValueType" minOccurs="0"/>
            <xs:element name="LastAmendedDate" type="base:LastAmendedDateType" minOccurs="0"/>
        </xs:all>
    </xs:complexType>

    <xs:complexType name="ManagedSerialNumbersType">
        <xs:sequence>
            <xs:element name="ManagedSerialNumber" type="target:ManagedSerialNumberType"
                minOccurs="0" maxOccurs="5"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ManagedSerialNumberType">
        <xs:sequence>
            <xs:element name="EnumerationId" type="target:NetworkSerialNumberIDType" minOccurs="1"/>
            <xs:element name="NetworkSerialNumber" type="base:NetworkSerialNumberType"
                minOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
	

    <xs:complexType name="ManagedSerialNumbersResponseType">
        <xs:sequence>
            <xs:element name="ManagedSerialNumber" type="target:ManagedSerialNumberResponseType"
                minOccurs="0" maxOccurs="5"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ManagedSerialNumberResponseType">
        <xs:complexContent>
            <xs:extension base="target:ManagedSerialNumberType">
                <xs:sequence>
                    <xs:element name="NetworkEffectiveDate" type="xs:date" minOccurs="0"/>
                    <xs:element name="NetworkCode" type="base:NetworkCodeType" minOccurs="0"/>
                    <xs:element name="NetworkSerialStatus" type="target:NetworkSerialStatusType"
                        minOccurs="0"/>
