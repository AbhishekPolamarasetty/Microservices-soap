<!-- This schema describes data types used by the Network Provisioning service. -->

<xs:schema targetNamespace="http://mdsuk.com/ws/dise3g/networkprovisioning/dto/business"
    xmlns:target="http://mdsuk.com/ws/dise3g/networkprovisioning/dto/business"
    xmlns:base="http://mdsuk.com/ws/dise3g/base" xmlns:xs="http://www.w3.org/2001/XMLSchema">


    <xs:import namespace="http://mdsuk.com/ws/dise3g/base" schemaLocation="base.xsd"/>


    <xs:simpleType name="TransactionIDType">
        <xs:restriction base="base:TenDigitNumberType"/>
    </xs:simpleType>
    
    <xs:simpleType name="TransactionStatusType">
        <xs:restriction base="base:StringMaxLength40Type"/>
    </xs:simpleType>
    
        <xs:simpleType name="ErrorCodeType">
        <xs:restriction base="base:StringMaxLength50Type"/>
    </xs:simpleType>
    
    <xs:simpleType name="ErrorDescriptionType">
        <xs:restriction base="base:StringMaxLength240NotRequiredType"/>
    </xs:simpleType>

    
</xs:schema>
