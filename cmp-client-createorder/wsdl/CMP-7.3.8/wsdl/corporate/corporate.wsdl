    <message name="QueryCorporateResponse">
        <part name="Parameters" element="target:QueryCorporateResponse"/>
    </message>

    <portType name="CorporatePortType">
        <operation name="CreateCorporate">
            <input message="target:CreateCorporateRequest"/>
            <output message="target:CreateCorporateResponse"/>
            <fault message="fault:SystemFault" name="SystemFault"/>
            <fault message="fault:SecurityFault" name="SecurityFault"/>
            <fault message="fault:ValidationFault" name="ValidationFault"/>
            <fault message="fault:ServiceUnavailableFault" name="ServiceUnavailableFault"/>
