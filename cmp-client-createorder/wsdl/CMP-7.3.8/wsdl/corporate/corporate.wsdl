            <element name="CreateCorporate">
                <complexType>
                    <sequence>
                        <element name="Request" type="corporate:CreateCorporateType"/>
                    </sequence>
                </complexType>
            </element>
            <element name="CreateCorporateResponse">
                <complexType>
                    <sequence>
                        <element name="Result" type="corporate:CreateCorporateResponseType"/>
                    </sequence>
                </complexType>
            </element>

            <element name="UpdateCorporate">
                <complexType>
                    <sequence>
                        <element name="Request" type="corporate:UpdateCorporateType"/>
                    </sequence>
                </complexType>
            </element>
            <element name="UpdateCorporateResponse">
