<?xml version="1.0" encoding="UTF-8"?>

<definitions targetNamespace="http://mdsuk.com/ws/dise3g/subscriptionfreeze/definition"
    xmlns="http://schemas.xmlsoap.org/wsdl/"
    xmlns:target="http://mdsuk.com/ws/dise3g/subscriptionfreeze/definition"
    xmlns:fault="http://mdsuk.com/ws/dise3g/fault/definition">

    <documentation>This Webservice definition encompasses all operations relating to subscription freeze.
    </documentation>
    
     <import namespace="http://mdsuk.com/ws/dise3g/fault/definition" location="../fault/fault.wsdl"/>

    <types>
        <schema xmlns="http://www.w3.org/2001/XMLSchema"
            targetNamespace="http://mdsuk.com/ws/dise3g/subscriptionfreeze/definition"
            xmlns:subscriptionfreeze="http://mdsuk.com/ws/dise3g/subscriptionfreeze/dto/service"
            elementFormDefault="qualified">

            <import namespace="http://mdsuk.com/ws/dise3g/subscriptionfreeze/dto/service" schemaLocation="../../xsd/subscriptionfreezeservice.xsd"/>

            <element name="FreezeSubscription">
                <complexType>
                    <sequence>
                        <element name="Request" type="subscriptionfreeze:FreezeSubscriptionType"/>
                    </sequence>
                </complexType>
            </element>
            
            <element name="FreezeSubscriptionResponse">
                <complexType>
                    <sequence>
                        <element name="Result" type="subscriptionfreeze:FreezeSubscriptionResponseType"/>
                    </sequence>
                </complexType>
            </element>
            
            <element name="UpdateFreeze">
                <complexType>
                    <sequence>
                        <element name="Request" type="subscriptionfreeze:UpdateFreezeType"/>
                    </sequence>
                </complexType>
            </element>
            
            <element name="UpdateFreezeResponse">
                <complexType>
                    <sequence>
                        <element name="Result" type="subscriptionfreeze:UpdateFreezeResponseType"/>
                    </sequence>
                </complexType>
            </element>

			<element name="CancelFreeze">
                <complexType>
                    <sequence>
                        <element name="Request" type="subscriptionfreeze:CancelFreezeType"/>
                    </sequence>
                </complexType>
            </element>
            
            <element name="CancelFreezeResponse">
                <complexType>
                    <sequence>
                        <element name="Result" type="subscriptionfreeze:CancelFreezeResponseType"/>
                    </sequence>
                </complexType>
            </element>
        </schema>
    </types>

    <message name="FreezeSubscriptionRequest">
        <part name="Parameters" element="target:FreezeSubscription"/>
    </message>

    <message name="FreezeSubscriptionResponse">
        <part name="Parameters" element="target:FreezeSubscriptionResponse"/>
    </message>
    
    <message name="UpdateFreezeRequest">
        <part name="Parameters" element="target:UpdateFreeze"/>
    </message>

    <message name="UpdateFreezeResponse">
        <part name="Parameters" element="target:UpdateFreezeResponse"/>
    </message>

    <message name="CancelFreezeRequest">
        <part name="Parameters" element="target:CancelFreeze"/>
    </message>

    <message name="CancelFreezeResponse">
        <part name="Parameters" element="target:CancelFreezeResponse"/>
    </message>

    <portType name="SubscriptionFreezePortType">
        <operation name="FreezeSubscription">
            <input message="target:FreezeSubscriptionRequest"/>
            <output message="target:FreezeSubscriptionResponse"/>
            <fault message="fault:SystemFault" name="SystemFault"/>
            <fault message="fault:SecurityFault" name="SecurityFault"/>
            <fault message="fault:ValidationFault" name="ValidationFault"/>
            <fault message="fault:ServiceUnavailableFault" name="ServiceUnavailableFault"/>
        </operation>
        <operation name="UpdateFreeze">
            <input message="target:UpdateFreezeRequest"/>
            <output message="target:UpdateFreezeResponse"/>
            <fault message="fault:SystemFault" name="SystemFault"/>
            <fault message="fault:SecurityFault" name="SecurityFault"/>
            <fault message="fault:ValidationFault" name="ValidationFault"/>
            <fault message="fault:ServiceUnavailableFault" name="ServiceUnavailableFault"/>
        </operation>
        <operation name="CancelFreeze">
            <input message="target:CancelFreezeRequest"/>
            <output message="target:CancelFreezeResponse"/>
            <fault message="fault:SystemFault" name="SystemFault"/>
            <fault message="fault:SecurityFault" name="SecurityFault"/>
            <fault message="fault:ValidationFault" name="ValidationFault"/>
            <fault message="fault:ServiceUnavailableFault" name="ServiceUnavailableFault"/>
        </operation>
    </portType>

</definitions>
