        </operation>

    </binding>

    <!-- service declns -->
    <service name="SubscriptionFreezePort">
        <documentation>This is the webservice for subscription freeze processing.</documentation>
        <port name="SubscriptionFreezePort" binding="target:SubscriptionFreezeSoapBinding">
            <soap:address location="http://localhost:63109/ws/dise3g/services/SubscriptionFreezePort"/>
        </port>
    </service>
</definitions>
