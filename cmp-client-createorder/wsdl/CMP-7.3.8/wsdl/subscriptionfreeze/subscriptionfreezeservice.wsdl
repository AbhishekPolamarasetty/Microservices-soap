<?xml version="1.0" encoding="UTF-8"?>

<definitions targetNamespace="http://mdsuk.com/ws/dise3g/subscriptionfreeze/service"
    xmlns="http://schemas.xmlsoap.org/wsdl/"
    xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
    xmlns:target="http://mdsuk.com/ws/dise3g/subscriptionfreeze/service"
    xmlns:definition="http://mdsuk.com/ws/dise3g/subscriptionfreeze/definition"
    xmlns:soapenc="http://schemas.xmlsoap.org/soap/encoding/">

    <import namespace="http://mdsuk.com/ws/dise3g/subscriptionfreeze/definition" location="subscriptionfreeze.wsdl"/>

    <!-- binding declns -->
    <binding name="SubscriptionFreezeSoapBinding" type="definition:SubscriptionFreezePortType">
