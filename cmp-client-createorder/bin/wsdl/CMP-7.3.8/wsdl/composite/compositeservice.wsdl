<?xml version="1.0" encoding="UTF-8"?>

<definitions targetNamespace="http://mdsuk.com/ws/dise3g/composite/service"
	xmlns="http://schemas.xmlsoap.org/wsdl/" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
	xmlns:target="http://mdsuk.com/ws/dise3g/composite/service"
	xmlns:definition="http://mdsuk.com/ws/dise3g/composite/definition"
	xmlns:soapenc="http://schemas.xmlsoap.org/soap/encoding/">

	<import namespace="http://mdsuk.com/ws/dise3g/composite/definition"
		location="composite.wsdl" />

	<!-- binding declns -->
	<binding name="CompositeSOAPBinding" type="definition:CompositePortType">
		<documentation> This Web service binding encompasses all operations relating to Dise3G
			ManageSubscriptionService WebServices.
		</documentation>
		<soap:binding style="document"
			transport="http://schemas.xmlsoap.org/soap/http" />
		<operation name="ManageSubscription">
			<documentation> Manage Subscription Services </documentation>
			<soap:operation />
			<input>
				<soap:body use="literal" />
			</input>
			<output>
				<soap:body use="literal" />
			</output>
			<fault name="SystemFault">
				<soap:fault use="literal" name="SystemFault" />
			</fault>
			<fault name="SecurityFault">
				<soap:fault use="literal" name="SecurityFault" />
			</fault>
			<fault name="ValidationFault">
				<soap:fault use="literal" name="ValidationFault" />
			</fault>
			<fault name="ServiceUnavailableFault">
				<soap:fault use="literal" name="ServiceUnavailableFault" />
			</fault>
		</operation>
		
	</binding>

	<!-- service declns -->
	<service name="CompositePort">
		<documentation> This is the Web Service for Dise3G service management.
		</documentation>
		<port name="CompositePort" binding="target:CompositeSOAPBinding">
			<soap:address
				location="http://localhost:63109/ws/dise3g/services/CompositePort" />
		</port>
	</service>

</definitions>