<definitions targetNamespace="http://mdsuk.com/ws/dise3g/corporate/service"
    xmlns="http://schemas.xmlsoap.org/wsdl/" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
    xmlns:target="http://mdsuk.com/ws/dise3g/corporate/service"
    xmlns:definition="http://mdsuk.com/ws/dise3g/corporate/definition"
    xmlns:soapenc="http://schemas.xmlsoap.org/soap/encoding/">

    <import namespace="http://mdsuk.com/ws/dise3g/corporate/definition" location="corporate.wsdl"/>

    <!-- binding declns -->
    <binding name="CorporateSoapBinding" type="definition:CorporatePortType">
        <documentation>
