            <fault name="ServiceUnavailableFault">
                <soap:fault name="ServiceUnavailableFault" use="literal"/>
            </fault>
        </operation>
        <operation name="QueryCorporate">
            <documentation>Corporate retrieval</documentation>
			<soap:operation/>
            <input>
                <soap:body use="literal"/>
            </input>
            <output>
                <soap:body use="literal"/>
            </output>
            <fault name="SystemFault">
                <soap:fault name="SystemFault" use="literal"/>
            </fault>
            <fault name="SecurityFault">
                <soap:fault name="SecurityFault" use="literal"/>
            </fault>
            <fault name="ValidationFault">
                <soap:fault name="ValidationFault" use="literal"/>
            </fault>
            <fault name="ServiceUnavailableFault">
                <soap:fault name="ServiceUnavailableFault" use="literal"/>
            </fault>
        </operation>
    </binding>

    <!-- service declns -->
    <service name="CorporatePort">
        <documentation>This is the webservice for DISE corporate processing.</documentation>
        <port name="CorporatePort" binding="target:CorporateSoapBinding">
            <soap:address location="http://localhost:9082/axis/services/CorporatePort"/>
        </port>
    </service>
</definitions>
