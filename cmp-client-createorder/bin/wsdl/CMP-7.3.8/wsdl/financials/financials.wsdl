<definitions targetNamespace="http://mdsuk.com/ws/dise3g/financials/definition"
    xmlns="http://schemas.xmlsoap.org/wsdl/"
    xmlns:target="http://mdsuk.com/ws/dise3g/financials/definition"
    xmlns:fault="http://mdsuk.com/ws/dise3g/fault/definition">
	
    <documentation>This Webservice definition encompasses all operations relating to Financials management.</documentation>

	<import namespace="http://mdsuk.com/ws/dise3g/fault/definition" location="../fault/fault.wsdl"/>

    <types>
        <schema xmlns="http://www.w3.org/2001/XMLSchema"
            targetNamespace="http://mdsuk.com/ws/dise3g/financials/definition"
            xmlns:financials="http://mdsuk.com/ws/dise3g/financials/dto/service"
            elementFormDefault="qualified">
            <import namespace="http://mdsuk.com/ws/dise3g/financials/dto/service" schemaLocation="../../xsd/financialsservice.xsd"/>

            <element name="QueryTransactions">
                <complexType>
                    <sequence>
                        <element name="TransactionData" type="financials:QueryTransactionsType"/>
                    </sequence>
                </complexType>
            </element>
            
            <element name="QueryTransactionsResponse">
                <complexType>
                    <sequence>
                        <element name="Transactions" type="financials:QueryTransactionsResponseType"/>
                    </sequence>
                </complexType>
            </element>
            
            <element name="QueryAccountBalance">
                <complexType>
                    <sequence>
                        <element name="AccountNumber" type="financials:QueryAccountBalanceType"/>
                    </sequence>
                </complexType>
            </element>
            
            <element name="QueryAccountBalanceResponse">
                <complexType>
                    <sequence>
                        <element name="BalanceDetails" type="financials:QueryAccountBalanceResponseType"/>
                    </sequence>
                </complexType>
            </element>  
            
            <element name="QueryInvoiceQueryAmounts">
                <complexType>
                    <sequence>
                        <element name="InvoiceQueryAmounts" type="financials:QueryInvoiceQueryAmountsType"/>
                    </sequence>
                </complexType>
            </element>
            
            <element name="QueryInvoiceQueryAmountsResponse">
                <complexType>
                    <sequence>
                        <element name="QueryInvoiceAmountsData" type="financials:QueryInvoiceQueryAmountsResponseType"/>
                    </sequence>
                </complexType>
            </element> 
            
            <element name="CreateInvoiceQueryAmount">
                <complexType>
                    <sequence>
                        <element name="CreatedInvQueryDetails" type="financials:CreateInvoiceQueryAmountType"/>
                    </sequence>
                </complexType>
            </element>
            
            
            <element name="CreateInvoiceQueryAmountResponse">
                <complexType>
                    <sequence>
                        <element name="CreatedInvQueryDetails" type="financials:CreateInvoiceQueryAmountResponseType"/>
                    </sequence>
                </complexType>
            </element>
            
           	<element name="QueryInvoices">
				<complexType>
	                <sequence>
	                    <element name="Request" type="financials:QueryInvoicesType"/>
					</sequence>
				</complexType>
			</element>
			
			<element name="QueryInvoicesResponse">
				<complexType>
					<sequence>
						<element name="Result" type="financials:QueryInvoicesResponseType" />
					</sequence>
				</complexType>
			</element>

			<element name="QueryInvoiceDetail">
				<complexType>
	                <sequence>
	                    <element name="Request" type="financials:QueryInvoiceDetailType"/>
					</sequence>
				</complexType>
			</element>
			
			<element name="QueryInvoiceDetailResponse">
				<complexType>
					<sequence>
						<element name="Result" type="financials:QueryInvoiceDetailResponseType" />
					</sequence>
				</complexType>
			</element>
           	
			<element name="QueryETC">
				<complexType>
	                <sequence>
	                    <element name="Request" type="financials:QueryETCType"/>
					</sequence>
				</complexType>
			</element>
			
			<element name="QueryETCResponse">
				<complexType>
					<sequence>
						<element name="Result" type="financials:QueryETCResponseType" />
					</sequence>
				</complexType>
			</element>
			
        </schema>
    </types>

    <message name="QueryTransactionsRequest">
        <part name="Parameters" element="target:QueryTransactions"/>
    </message>

    <message name="QueryTransactionsResponse">
        <part name="Parameters" element="target:QueryTransactionsResponse"/>
    </message>

    <message name="QueryAccountBalanceRequest">
        <part name="Parameters" element="target:QueryAccountBalance"/>
    </message>
    
    <message name="QueryAccountBalanceResponse">
        <part name="Parameters" element="target:QueryAccountBalanceResponse"/>
    </message>
    
   	<message name="QueryInvoiceQueryAmountsRequest">
        <part name="Parameters" element="target:QueryInvoiceQueryAmounts"/>
    </message>

    <message name="QueryInvoiceQueryAmountsResponse">
        <part name="Parameters" element="target:QueryInvoiceQueryAmountsResponse"/>
    </message>

    <message name="CreateInvoiceQueryAmountRequest">
        <part name="Parameters" element="target:CreateInvoiceQueryAmount"/>
    </message>

    <message name="CreateInvoiceQueryAmountResponse">
        <part name="Parameters" element="target:CreateInvoiceQueryAmountResponse"/>
    </message>
    	
	<message name="QueryInvoicesRequest">
		<part name="Parameters" element="target:QueryInvoices" />
	</message>

	<message name="QueryInvoicesResponse">
		<part name="Parameters" element="target:QueryInvoicesResponse" />
	</message>
	
	<message name="QueryInvoiceDetailRequest">
		<part name="Parameters" element="target:QueryInvoiceDetail" />
	</message>

	<message name="QueryInvoiceDetailResponse">
		<part name="Parameters" element="target:QueryInvoiceDetailResponse" />
	</message>

 	<message name="QueryETCRequest">
		<part name="Parameters" element="target:QueryETC" />
	</message>

	<message name="QueryETCResponse">
		<part name="Parameters" element="target:QueryETCResponse" />
	</message>
	
    <portType name="FinancialsPortType">
        <operation name="QueryTransactions">
            <input message="target:QueryTransactionsRequest"/>
            <output message="target:QueryTransactionsResponse"/>
            <fault message="fault:SystemFault" name="SystemFault"/>
            <fault message="fault:SecurityFault" name="SecurityFault"/>
            <fault message="fault:ValidationFault" name="ValidationFault"/>
            <fault message="fault:ServiceUnavailableFault" name="ServiceUnavailableFault"/>
        </operation>
        <operation name="QueryAccountBalance">
            <input message="target:QueryAccountBalanceRequest"/>
            <output message="target:QueryAccountBalanceResponse"/>
            <fault message="fault:SystemFault" name="SystemFault"/>
            <fault message="fault:SecurityFault" name="SecurityFault"/>
            <fault message="fault:ValidationFault" name="ValidationFault"/>
            <fault message="fault:ServiceUnavailableFault" name="ServiceUnavailableFault"/>
        </operation>   
         <operation name="QueryInvoiceQueryAmounts">
            <input message="target:QueryInvoiceQueryAmountsRequest"/>
            <output message="target:QueryInvoiceQueryAmountsResponse"/>
            <fault message="fault:SystemFault" name="SystemFault"/>
            <fault message="fault:SecurityFault" name="SecurityFault"/>
            <fault message="fault:ValidationFault" name="ValidationFault"/>
            <fault message="fault:ServiceUnavailableFault" name="ServiceUnavailableFault"/>
        </operation>
        <operation name="CreateInvoiceQueryAmount">
            <input message="target:CreateInvoiceQueryAmountRequest"/>
            <output message="target:CreateInvoiceQueryAmountResponse"/>
            <fault message="fault:SystemFault" name="SystemFault"/>
            <fault message="fault:SecurityFault" name="SecurityFault"/>
            <fault message="fault:ValidationFault" name="ValidationFault"/>
            <fault message="fault:ServiceUnavailableFault" name="ServiceUnavailableFault"/>
        </operation>

		<operation name="QueryInvoices">
			<input message="target:QueryInvoicesRequest" />
			<output message="target:QueryInvoicesResponse" />
			<fault message="fault:SystemFault" name="SystemFault" />
			<fault message="fault:SecurityFault" name="SecurityFault" />
			<fault message="fault:ValidationFault" name="ValidationFault" />
			<fault message="fault:ServiceUnavailableFault" name="ServiceUnavailableFault" />
		</operation>
		
		<operation name="QueryInvoiceDetail">
			<input message="target:QueryInvoiceDetailRequest" />
			<output message="target:QueryInvoiceDetailResponse" />
			<fault message="fault:SystemFault" name="SystemFault" />
			<fault message="fault:SecurityFault" name="SecurityFault" />
			<fault message="fault:ValidationFault" name="ValidationFault" />
			<fault message="fault:ServiceUnavailableFault" name="ServiceUnavailableFault" />
		</operation>
		
		<operation name="QueryETC">
			<input message="target:QueryETCRequest" />
			<output message="target:QueryETCResponse" />
			<fault message="fault:SystemFault" name="SystemFault" />
			<fault message="fault:SecurityFault" name="SecurityFault" />
			<fault message="fault:ValidationFault" name="ValidationFault" />
			<fault message="fault:ServiceUnavailableFault" name="ServiceUnavailableFault" />
		</operation>		
