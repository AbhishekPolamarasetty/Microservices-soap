<!-- This schema contains data types used to describe Tariffs -->

<xs:schema targetNamespace="http://mdsuk.com/ws/dise3g/tariff/dto/business"
    xmlns:target="http://mdsuk.com/ws/dise3g/tariff/dto/business"
    xmlns:base="http://mdsuk.com/ws/dise3g/base"
    xmlns:subscription="http://mdsuk.com/ws/dise3g/subscription/dto/business"
    xmlns:xs="http://www.w3.org/2001/XMLSchema">

    <xs:import namespace="http://mdsuk.com/ws/dise3g/base" schemaLocation="base.xsd"/>
    <xs:import namespace="http://mdsuk.com/ws/dise3g/subscription/dto/business" schemaLocation="subscription.xsd"/>

    <xs:complexType name="TariffType">
        <xs:all>
            <xs:element name="Company" type="base:CompanyCodeType" minOccurs="0"/>
            <xs:element name="TariffCode" type="target:TariffCodeType" minOccurs="0"/>
            <xs:element name="EventTypeCode" type="target:EventTypeCodeType" minOccurs="0"/>
            <xs:element name="EventCode" type="target:EventCodeType" minOccurs="0"/>
            <xs:element name="NetworkCode" type="base:NetworkCodeType" minOccurs="0"/>
            <xs:element name="CallClassCode" type="target:CallClassCodeType" minOccurs="0"/>
            <xs:element name="NetworkTypeCode" type="target:NetworkTypeCodeType" minOccurs="0"/>
            <xs:element name="TermConditionGroupCode" type="target:TermConditionGroupCodeType" minOccurs="0"/>
            <xs:element name="TCGEffectiveDate" type="target:TCGEffectiveDateType" minOccurs="0"/>
            <xs:element name="BillingMediaCode" type="target:BillingMediaCodeType" minOccurs="0"/>
            <xs:element name="CostCenterCode" type="target:CostCenterCodeType" minOccurs="0"/>
            <xs:element name="DailyUsageLimit" type="target:DailyUsageLimitType" minOccurs="0"/>
            <xs:element name="DefContractTermMnthsNumber" type="target:DefContractTermMnthsNumberType" minOccurs="0"/>
            <xs:element name="LowerUsageLimit" type="target:LowerUsageLimitType" minOccurs="0"/>
            <xs:element name="UpperUsageLimit" type="target:UpperUsageLimitType" minOccurs="0"/>
            <xs:element name="TariffDescription" type="target:TariffDescriptionType" minOccurs="0"/>
            <xs:element name="DefaulttoNetworkPrice" type="target:DefaulttoNetworkPriceType" minOccurs="0"/>
            <xs:element name="UseNetworkPriceasMin" type="target:UseNetworkPriceasMinType" minOccurs="0"/>
            <xs:element name="MinDaysAfterSubsStartNumber" type="target:MinDaysAfterSubsStartNumberType" minOccurs="0"/>
            <xs:element name="MinDaysAfterSubsTerminationNumber" type="target:MinDaysAfterSubsTerminationNumberType" minOccurs="0"/>
            <xs:element name="ApplyFirstPeriodFree" type="target:ApplyFirstPeriodFreeType" minOccurs="0"/>
            <xs:element name="OngoingInvFreqDaysNumber" type="target:OngoingInvFreqDaysNumberType" minOccurs="0"/>
            <xs:element name="OngoingInvFreqMonthsNumber" type="target:OngoingInvFreqMonthsNumberType" minOccurs="0"/>
            <xs:element name="CPPublicHolidayDay" type="target:CPPublicHolidayDayType" minOccurs="0"/>
            <xs:element name="NoticePeriodMonthsNumber" type="target:NoticePeriodMonthsNumberType" minOccurs="0"/>
            <xs:element name="MaxTotalTerminationFee" type="target:MaxTotalTerminationFeeType" minOccurs="0"/>
            <xs:element name="TerminationFreeDaysNumber" type="target:TerminationFreeDaysNumberType" minOccurs="0"/>
            <xs:element name="MinDaysFirstNormalNumber" type="target:MinDaysFirstNormalNumberType" minOccurs="0"/>
            <xs:element name="MinDaysNormalFinalNumber" type="target:MinDaysNormalFinalNumberType" minOccurs="0"/>
            <xs:element name="IncludeFutureServices" type="target:IncludeFutureServicesType" minOccurs="0"/>
            <xs:element name="IncludeFutureDiscounts" type="target:IncludeFutureDiscountsType" minOccurs="0"/>
            <xs:element name="MaximumCallPrice" type="target:MaximumCallPriceType" minOccurs="0"/>
            <xs:element name="ExplanationText1" type="target:ExplanationText1Type" minOccurs="0"/>
            <xs:element name="ExplanationText2" type="target:ExplanationText2Type" minOccurs="0"/>
            <xs:element name="ExplanationText3" type="target:ExplanationText3Type" minOccurs="0"/>
            <xs:element name="ExplanationText4" type="target:ExplanationText4Type" minOccurs="0"/>
        </xs:all>
    </xs:complexType>
    <xs:complexType name="TariffIdType">
        <xs:all>
            <xs:element name="Company" type="base:CompanyCodeType" minOccurs="1"/>
            <xs:element name="TariffCode" type="target:TariffCodeType" minOccurs="1"/>
        </xs:all>
    </xs:complexType>
    <xs:complexType name="TariffUpdateContractAndSalesType">
        <xs:annotation>
            <xs:documentation>Update the Contract And Sales data associated with a Subscription - TB
                check these are stored on Sub file rather than agreement file!!!</xs:documentation>
        </xs:annotation>
        <xs:all>
            <xs:element name="ContractNumber" type="subscription:ContractNumberType" minOccurs="0"/>
            <xs:element name="ContractStartDate" type="base:ClearableDateType"
                minOccurs="0"/>
            <xs:element name="TermOfContract" type="subscription:ContractTermType" minOccurs="0"/>
            <xs:element name="TermsAndConditionsGroupCode"
                type="subscription:TermsAndConditionsGroupCodeType" minOccurs="0"/>
            <xs:element name="CarriedOverContractDays" type="base:ThreeDigitNumberType" minOccurs="0"/>
        </xs:all>
    </xs:complexType>

    <xs:simpleType name="TariffCodeType">
        <xs:restriction base="base:StringMaxLength6NotRequiredType"/>
    </xs:simpleType>
    <xs:simpleType name="EventTypeCodeType">
        <xs:restriction base="base:StringMaxLength4Type"/>
    </xs:simpleType>
    <xs:simpleType name="EventCodeType">
        <xs:restriction base="base:StringMaxLength4Type"/>
    </xs:simpleType>
    <xs:simpleType name="CallClassCodeType">
        <xs:restriction base="xs:string">
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="NetworkTypeCodeType">
        <xs:restriction base="base:StringMaxLength6NotRequiredType"/>
    </xs:simpleType>
    <xs:simpleType name="TermConditionGroupCodeType">
        <xs:restriction base="base:StringMaxLength6NotRequiredType"/>
    </xs:simpleType>
     <xs:simpleType name="TCGEffectiveDateType">
        <xs:restriction base="base:Decimal7Type"/>
    </xs:simpleType>
    <xs:simpleType name="BillingMediaCodeType">
        <xs:restriction base="base:StringMaxLength6NotRequiredType"/>
    </xs:simpleType>
    <xs:simpleType name="CostCenterCodeType">
        <xs:restriction base="base:StringMaxLength12Type"/>
    </xs:simpleType>
    <xs:simpleType name="DailyUsageLimitType">
        <xs:restriction base="base:Currency9_2Type"/>
    </xs:simpleType>
    <xs:simpleType name="DefContractTermMnthsNumberType">
        <xs:restriction base="base:Decimal3Type"/>
    </xs:simpleType>
    <xs:simpleType name="LowerUsageLimitType">
        <xs:restriction base="base:Currency9_2Type"/>
    </xs:simpleType>
    <xs:simpleType name="UpperUsageLimitType">
        <xs:restriction base="base:Currency9_2Type"/>
    </xs:simpleType>
    <xs:simpleType name="TariffDescriptionType">
         <xs:restriction base="xs:string">
         </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="DefaulttoNetworkPriceType">
        <xs:restriction base="base:StringMaxLength1Type"/>
    </xs:simpleType>
    <xs:simpleType name="UseNetworkPriceasMinType">
        <xs:restriction base="base:StringMaxLength1Type"/>
    </xs:simpleType>
    <xs:simpleType name="MinDaysAfterSubsStartNumberType">
        <xs:restriction base="base:Decimal3Type"/>
    </xs:simpleType>
    <xs:simpleType name="MinDaysAfterSubsTerminationNumberType">
        <xs:restriction base="base:Decimal3Type"/>
    </xs:simpleType>
    <xs:simpleType name="ApplyFirstPeriodFreeType">
        <xs:restriction base="base:StringMaxLength1Type"/>
    </xs:simpleType>
    <xs:simpleType name="OngoingInvFreqDaysNumberType">
        <xs:restriction base="base:Decimal3Type"/>
    </xs:simpleType>
    <xs:simpleType name="OngoingInvFreqMonthsNumberType">
        <xs:restriction base="base:Decimal2Type"/>
    </xs:simpleType>
    <xs:simpleType name="CPPublicHolidayDayType">
        <xs:restriction base="base:StringMaxLength1Type"/>
    </xs:simpleType>
    <xs:simpleType name="NoticePeriodMonthsNumberType">
        <xs:restriction base="base:Decimal3Type"/>
    </xs:simpleType>
    <xs:simpleType name="MaxTotalTerminationFeeType">
        <xs:restriction base="base:Currency9_2Type"/>
    </xs:simpleType>
    <xs:simpleType name="TerminationFreeDaysNumberType">
        <xs:restriction base="base:Decimal3Type"/>
    </xs:simpleType>
    <xs:simpleType name="MinDaysFirstNormalNumberType">
        <xs:restriction base="base:Decimal3Type"/>
    </xs:simpleType>
    <xs:simpleType name="MinDaysNormalFinalNumberType">
        <xs:restriction base="base:Decimal3Type"/>
    </xs:simpleType>
    <xs:simpleType name="IncludeFutureServicesType">
        <xs:restriction base="base:StringMaxLength1Type"/>
    </xs:simpleType>
    <xs:simpleType name="IncludeFutureDiscountsType">
        <xs:restriction base="base:StringMaxLength1Type"/>
    </xs:simpleType>
    <xs:simpleType name="Currency7_4Type">
        <xs:restriction base="xs:decimal">
            <xs:minInclusive value="0.0001"/>
            <xs:maxInclusive value="9999999.9999"/>
            <xs:fractionDigits value="4"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="MaximumCallPriceType">
        <xs:restriction base="target:Currency7_4Type"/>
    </xs:simpleType>
    <xs:simpleType name="ExplanationText1Type">
         <xs:restriction base="xs:string">
         </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ExplanationText2Type">
         <xs:restriction base="xs:string">
         </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ExplanationText3Type">
         <xs:restriction base="xs:string">
         </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ExplanationText4Type">
         <xs:restriction base="xs:string">
         </xs:restriction>
    </xs:simpleType>
    

    
   
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    


    
    
    
    
</xs:schema>
