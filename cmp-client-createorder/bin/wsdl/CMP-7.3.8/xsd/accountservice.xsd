<!-- This schema describes the API data types for the Account service -->
<xs:schema targetNamespace="http://mdsuk.com/ws/dise3g/account/dto/service"
    xmlns:target="http://mdsuk.com/ws/dise3g/account/dto/service"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:account="http://mdsuk.com/ws/dise3g/account/dto/business"
    xmlns:address="http://mdsuk.com/ws/dise3g/address/dto/business"
    xmlns:discount="http://mdsuk.com/ws/dise3g/discount/dto/business"
    xmlns:subscription="http://mdsuk.com/ws/dise3g/subscription/dto/business"
    xmlns:payment="http://mdsuk.com/ws/dise3g/payment/dto/business"
    xmlns:financials="http://mdsuk.com/ws/dise3g/financials/dto/business"
	xmlns:service="http://mdsuk.com/ws/dise3g/services/dto/business"
	xmlns:namesfornumbers="http://mdsuk.com/ws/dise3g/namesfornumbers/dto/business"
    xmlns:base="http://mdsuk.com/ws/dise3g/base"
    xmlns:financialsservice="http://mdsuk.com/ws/dise3g/financials/dto/service"
    xmlns:billing="http://mdsuk.com/ws/dise3g/billing/dto/business">
    <xs:import namespace="http://mdsuk.com/ws/dise3g/account/dto/business"
        schemaLocation="account.xsd"/>
            <xs:import namespace="http://mdsuk.com/ws/dise3g/subscription/dto/business"
        schemaLocation="subscription.xsd"/>
    <xs:import namespace="http://mdsuk.com/ws/dise3g/discount/dto/business"
        schemaLocation="discount.xsd"/>
    <xs:import namespace="http://mdsuk.com/ws/dise3g/address/dto/business"
        schemaLocation="address.xsd"/>
    <xs:import namespace="http://mdsuk.com/ws/dise3g/payment/dto/business"
        schemaLocation="payment.xsd"/>
    <xs:import namespace="http://mdsuk.com/ws/dise3g/financials/dto/business"
        schemaLocation="financials.xsd"/>
	<xs:import namespace="http://mdsuk.com/ws/dise3g/services/dto/business"
		schemaLocation="services.xsd"/>
    <xs:import namespace="http://mdsuk.com/ws/dise3g/namesfornumbers/dto/business"
        schemaLocation="namesfornumbers.xsd"/>		
    <xs:import namespace="http://mdsuk.com/ws/dise3g/base" schemaLocation="base.xsd"/>
    <xs:import namespace="http://mdsuk.com/ws/dise3g/financials/dto/service"
        schemaLocation="financialsservice.xsd"/>
    <xs:import namespace="http://mdsuk.com/ws/dise3g/billing/dto/business"
        schemaLocation="billing.xsd" />

    <xs:complexType name="CreateAccountType">
        <xs:all>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/>
            <xs:element name="AccountData" type="account:AccountType" minOccurs="1"/>
        </xs:all>
    </xs:complexType>

    <xs:complexType name="CreateAccountResponseType">
        <xs:all>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/>
            <xs:element name="AccountNumber" type="base:AccountNumberType"/>
        </xs:all>
    </xs:complexType>
    
    <xs:complexType name="CreateAccountSerialNumberType">
        <xs:sequence>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/> 
            <xs:element name="AccountNumber" type="base:AccountNumberType" minOccurs="1"/>
			<xs:element name="SerialNumberId" type="account:AccountSerialNumberIdType" minOccurs="1"/>
            <xs:element name="SerialNumberValue" type="base:NetworkSerialNumberType" minOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="CreateAccountSerialNumberResponseType">
        <xs:all>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/>
        </xs:all>
    </xs:complexType>
    
    <xs:complexType name="SetAccountSerialNumberType">
        <xs:sequence>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/> 
            <xs:element name="AccountNumber" type="base:AccountNumberType" minOccurs="1"/>
			<xs:element name="SerialNumberId" type="account:AccountSerialNumberIdType" minOccurs="1"/>
            <xs:element name="SerialNumberValue" type="base:NetworkSerialNumberType" minOccurs="1"/>
			<xs:element name="SetOnHighestAccount" type="xs:boolean" minOccurs="0"/>
			<xs:element name="AuditRecord" type="base:AuditRecordType" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="SetAccountSerialNumberResponseType">
        <xs:all>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/>
        </xs:all>
    </xs:complexType>

    <xs:complexType name="QueryAccountType">
        <xs:sequence>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/>
            <xs:element name="AccountNumber" type="base:AccountNumberType" minOccurs="1"/>
            <xs:element name="Datasets" type="account:QueryAccountDatasetsType" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="QueryAccountResponseType">
        <xs:sequence>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/>
            <xs:element name="AccountBasic" type="account:AccountBasicType" minOccurs="0"/>
            <xs:element name="Discounts" type="discount:DiscountsType" minOccurs="0"/>
            <xs:element name="PaymentDetails" type="account:PaymentDetailsType" minOccurs="0"/>
            <xs:element name="Address" type="address:AddressQueryType" minOccurs="0"/>
            <xs:element name="AutoPayments" type="account:AutoPaymentsType" minOccurs="0"/>
            <xs:element name="CreditControl" type="account:CreditControlType" minOccurs="0"/>
            <xs:element name="Bonds" type="account:QueryAccountBondsType" minOccurs="0"/>
            <xs:element name="AccountTypeAndUsage" type="account:AccountTypeAndUsageType"
                minOccurs="0"/>
            <xs:element name="AccountSerialNumbers" type="account:AccountSerialNumbersType"
                minOccurs="0"/>
            <xs:element name="CustomerDetails" type="account:CustomerDetailsType" minOccurs="0"/>
            <xs:element name="SubscriptionInvoiceControl"
                type="account:SubscriptionInvoiceControlType" minOccurs="0"/>
            <xs:element name="NonSubscriptionInvoiceControl"
                type="account:NonSubscriptionInvoiceControlType" minOccurs="0"/>
            <xs:element name="Services" type="service:ServiceDetailsType" minOccurs="0"/>
            <xs:element name="AttributeGroup" type="billing:QueryAttributeGroupType" minOccurs="0"/>
            <xs:element name="CreditDebitCards" type="financials:CreditDebitCardsType" minOccurs="0"/>
            <xs:element name="LastAmendedDate" type="base:LastAmendedDateType" minOccurs="0"/>
            <xs:element name="TerminationFee" type="account:TerminationFeeType" minOccurs="0"/>
            <xs:element name="RewardEntitlement" type="subscription:RewardEntitlementsTypes" minOccurs="0"/>
			<xs:element name="DLPPorts" type="account:DlpPortsType" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="UpdateRecurringPaymentDetailsType">
        <xs:all>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/>
            <xs:element name="AccountNumber" type="base:AccountNumberType" minOccurs="1"/>
            <xs:element name="PaymentType" type="base:PaymentTypeType" minOccurs="1"/>
            <xs:element name="PaymentTerm" type="base:PaymentTermType" minOccurs="0"/>
            <xs:element name="PaymentDetails" type="account:UpdatePaymentDetailsType" minOccurs="0"/>
            <xs:element name="LastAmendedDate" type="base:LastAmendedDateType" minOccurs="1"/>
			<xs:element name="AuditRecord" type="base:AuditRecordType" minOccurs="0"/>
        </xs:all>
    </xs:complexType>

    <xs:complexType name="UpdateRecurringPaymentDetailsResponseType">
        <xs:all>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/>
        </xs:all>
    </xs:complexType>
	
	<xs:complexType name="SetCustomerPreferencesType">
        <xs:all>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/>
            <xs:element name="AccountNumber" type="base:AccountNumberType" minOccurs="1"/>
			<xs:element name="CommunicationMethod" type="account:CustomerPreferenceOptionType" minOccurs="0"/>
			<xs:element name="ParticularRequirement" type="account:CustomerPreferenceOptionType" minOccurs="0"/>
			<xs:element name="Language" type="base:StringMaxLength2Type" minOccurs="0"/>
			<xs:element name="MarketingByLetter" type="xs:boolean" minOccurs="0"/>
			<xs:element name="MarketingByEmail" type="xs:boolean" minOccurs="0"/>
			<xs:element name="MarketingByTelephone" type="xs:boolean" minOccurs="0"/>
			<xs:element name="MarketingBySMS" type="xs:boolean" minOccurs="0"/>
			<xs:element name="MarketingOnInvoiceStatement" type="xs:boolean" minOccurs="0"/>
			<xs:element name="AuditRecord" type="base:AuditRecordType" minOccurs="0"/>
        </xs:all>
    </xs:complexType>

    <xs:complexType name="SetCustomerPreferencesResponseType">
        <xs:all>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/>
        </xs:all>
    </xs:complexType>

    <xs:complexType name="QueryAccountTransactionsType">
        <xs:all>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/>
            <xs:element name="AccountNumber" type="base:AccountNumberType" minOccurs="1"/>
            <xs:element name="AccountTransactionsFilterType"
                type="financialsservice:TransactionsFilterType" minOccurs="1"/>
        </xs:all>
    </xs:complexType>

    <xs:complexType name="QueryAccountTransactionsResponseType">
        <xs:all>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/>
            <xs:element name="QueryAccountTransactionsList"
                type="account:AccountTransactionsListResponseType" minOccurs="0"/>
            <xs:element name="LastAmendedDate" type="base:LastAmendedDateType" minOccurs="0"/>
        </xs:all>
    </xs:complexType>
	
	<xs:complexType name="QueryCustomerPreferencesType">
        <xs:all>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/>
            <xs:element name="AccountNumber" type="base:AccountNumberType" minOccurs="1"/>
        </xs:all>
    </xs:complexType>

    <xs:complexType name="QueryCustomerPreferencesResponseType">
        <xs:all>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/>
            <xs:element name="CommunicationPreferences" type="account:CommunicationPreferencesType" minOccurs="1"/>
        </xs:all>
    </xs:complexType>

    <xs:complexType name="UpdateAccountDetailsType">
        <xs:sequence>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/>
            <xs:element name="AccountNumber" type="base:AccountNumberType" minOccurs="1"/>
            <xs:element name="UpdateAccountBasic" type="account:UpdateAccountBasic" minOccurs="0"/>
            <xs:element name="UpdateAccountTypeAndUsage" type="account:UpdateAccountTypeAndUsage"
                minOccurs="0"/>
            <xs:element name="UpdateCustomerDetails" type="account:UpdateCustomerDetailsType"
                minOccurs="0"/>
            <xs:element name="UpdateCreditControl" type="account:UpdateCreditControlType"
                minOccurs="0"/>
            <xs:element name="UpdateSubscriptionInvoiceControl"
                type="account:UpdateSubscriptionInvoiceControlType" minOccurs="0"/>
            <xs:element name="UpdateNonSubscriptionInvoiceControl"
                type="account:UpdateNonSubscriptionInvoiceControlType" minOccurs="0"/>
            <xs:element name="UpdateAutoPayments" type="account:UpdateAutoPaymentsType"
                minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="UpdateAccountDetailsResponseType">
        <xs:sequence>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
        
    <xs:complexType name="UpdateAccountSerialNumberType">
        <xs:sequence>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/>
            <xs:element name="AccountNumber" type="base:AccountNumberType" minOccurs="1"/>
            <xs:element name="SerialNumberId" type="account:AccountSerialNumberIdType" minOccurs="1"/>
            <xs:element name="SerialNumberValue" type="base:NetworkSerialNumberType" minOccurs="1"/>
			<xs:element name="LastAmendedDate" type="base:LastAmendedDateType" minOccurs="1"/> 
        </xs:sequence>
    </xs:complexType>
   
	<xs:complexType name="UpdateAccountSerialNumberResponseType">
        <xs:sequence>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

	<xs:complexType name="QueryNamesForNumbersType">
        <xs:sequence>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/>
            <xs:element name="AccountIdentifier" type="base:AccountNumberType" minOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
	
    <xs:complexType name="QueryNamesForNumbersResponseType">
        <xs:sequence>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/>
			<xs:element name="QueryLimitExceeded" type="base:QueryLimitExceededType" minOccurs="1"/>
			<xs:element name="NamesForNumbers" type="namesfornumbers:NamesForNumbersResponseType" minOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
	
    <xs:complexType name="SetNamesForNumbersListType">
        <xs:sequence>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/>
            <xs:element name="AccountIdentifier" type="base:AccountNumberType" minOccurs="1"/>
			<xs:element name="NamesForNumbers" type="namesfornumbers:NamesForNumbersType" minOccurs="1"/>			
       </xs:sequence>
    </xs:complexType>

    <xs:complexType name="SetNamesForNumbersListResponseType">
        <xs:sequence>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="RecordBondPaymentType">
        <xs:sequence>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/>
            <xs:element name="AccountNumber" type="base:AccountNumberType" minOccurs="1"/>
			<xs:element name="Bond" type="account:CreateAccountBondType" minOccurs="1"/>			
			<xs:element name="AuditRecord" type="base:AuditRecordType" minOccurs="0"/>
       </xs:sequence>
    </xs:complexType>

    <xs:complexType name="RecordBondPaymentResponseType">
        <xs:sequence>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/>
            <xs:element name="BondNumber" type="base:BondNumberType" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="AddRegisteredCardType">
        <xs:sequence>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/>
            <xs:element name="AccountNumber" type="base:AccountNumberType" minOccurs="1"/>
            <xs:element name="AddRegisteredCardData" type="target:AddRegisteredCardDataType" minOccurs="0"/>
			<xs:element name="Card" type="account:AddRegisteredCardType" minOccurs="1"/>
			<xs:element name="CancelExistingCard" type="xs:boolean" minOccurs="0"/>	
			<xs:element name="CancellationReason" type="xs:string" minOccurs="0"/>  
			<xs:element name="AuditRecord" type="base:AuditRecordType" minOccurs="0"/>
       </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="AddRegisteredCardDataType">
        <xs:choice>
            <xs:element name="SubscriptionNumber" type="base:SubscriptionNumberType" maxOccurs="1"/>
            <xs:element name="NetworkSerialNumber" type="base:NetworkSerialNumberType" maxOccurs="1"/>
        </xs:choice>
    </xs:complexType>

    <xs:complexType name="AddRegisteredCardResponseType">
        <xs:sequence>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>  
	   
    <xs:complexType name="CancelRegisteredCardType">
        <xs:sequence>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/>
            <xs:element name="TargetIdentifier" type="account:RegisteredCardTargetIdentifierType" minOccurs="1"/>            
			<xs:element name="Card" type="account:CancelRegisteredCardType" minOccurs="1"/>			
			<xs:element name="AuditRecord" type="base:AuditRecordType" minOccurs="0"/>
       </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="CancelRegisteredCardResponseType">
        <xs:sequence>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
</xs:schema>
