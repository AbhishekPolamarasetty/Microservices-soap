<!-- This schema describes data types used by the Security service. -->
<xs:schema targetNamespace="http://mdsuk.com/ws/dise3g/security/dto/business"
    xmlns:target="http://mdsuk.com/ws/dise3g/security/dto/business"
    xmlns:base="http://mdsuk.com/ws/dise3g/base"
    xmlns:xs="http://www.w3.org/2001/XMLSchema">
	
	<xs:import namespace="http://mdsuk.com/ws/dise3g/base" schemaLocation="base.xsd"></xs:import>
	
	
	<xs:complexType name="SessionAttributeType">
		<xs:sequence>
		    <xs:element name="AttributeId" type="target:AttributeIdType"/>
			<xs:element name="AttributeValue" type="target:AttributeValueType"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="SessionAttributesType">
		<xs:sequence>
		    <xs:element name="Attribute" type="target:SessionAttributeType" maxOccurs="unbounded" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:simpleType name="AttributeIdType">
        <xs:restriction base="xs:integer">
			<xs:minInclusive value="1"></xs:minInclusive>
			<xs:maxInclusive value="99"></xs:maxInclusive>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="AttributeValueType">
        <xs:restriction base="base:StringMaxLength30NotRequiredType"/>
    </xs:simpleType>
	
</xs:schema>
