<!-- This schema describes the API data types for the Agreement service -->
<xs:schema targetNamespace="http://mdsuk.com/ws/dise3g/simulationworkfile/dto/service"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:simulationworkfile="http://mdsuk.com/ws/dise3g/simulationworkfile/dto/business"
    xmlns:base="http://mdsuk.com/ws/dise3g/base"
    xmlns:target="http://mdsuk.com/ws/dise3g/simulationworkfile/dto/service"
    xmlns:service="http://mdsuk.com/ws/dise3g/services/dto/business"
    xmlns:bundle="http://mdsuk.com/ws/dise3g/bundle/dto/business">

    <xs:import namespace="http://mdsuk.com/ws/dise3g/simulationworkfile/dto/business"
        schemaLocation="simulationworkfile.xsd"/>
    <xs:import namespace="http://mdsuk.com/ws/dise3g/base" schemaLocation="base.xsd"/>
    <xs:import namespace="http://mdsuk.com/ws/dise3g/services/dto/business"
        schemaLocation="services.xsd"/>
    <xs:import namespace="http://mdsuk.com/ws/dise3g/bundle/dto/business"
        schemaLocation="bundle.xsd"/>

    <xs:complexType name="SubscriptionNumber">
        <xs:all>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/>
            <xs:element name="SubscriptionNumberData"
                type="simulationworkfile:SubscriptionNumberType" minOccurs="1"/>
        </xs:all>
    </xs:complexType>

    <xs:complexType name="SimulationWorkfileDataResponseType">
        <xs:sequence>
            <xs:element name="ExternalReference" type="base:ExternalReferenceType" minOccurs="0"/>
            <xs:element name="SimulationWorkfile" type="simulationworkfile:SimulationWorkfileType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

</xs:schema>
