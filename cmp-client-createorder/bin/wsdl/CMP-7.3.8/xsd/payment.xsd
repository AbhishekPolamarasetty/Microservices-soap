<!-- This schema describes data types used by the Payments service. -->

<xs:schema targetNamespace="http://mdsuk.com/ws/dise3g/payment/dto/business"
    xmlns:base="http://mdsuk.com/ws/dise3g/base"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:target="http://mdsuk.com/ws/dise3g/payment/dto/business">

    <xs:import namespace="http://mdsuk.com/ws/dise3g/base" schemaLocation="base.xsd"/>

    <xs:complexType name="PaymentType">
        <xs:sequence>
            <xs:element name="paymentTypeCode" type="xs:string" minOccurs="1"/>
            <xs:element name="accountNumber" type="base:AccountNumberType" minOccurs="1"/>
            <xs:element name="cardNumber" type="xs:integer" minOccurs="1"/>
            <xs:element name="nameOnCard" type="xs:string" minOccurs="0" nillable="true"/>
            <xs:element name="startDateMMYYNumber" type="target:MonthYearType" minOccurs="0"
                nillable="true"/>
            <xs:element name="expiryDateMMYYNumber" type="target:MonthYearType" minOccurs="1"/>
            <xs:element name="amount" type="base:Currency9_2Type" minOccurs="1"/>
            <xs:element name="issueNumber" type="base:OneOrTwoDigitNumberType" minOccurs="0"/>
            <xs:element name="AVSHouseStreetNumber" type="xs:string" minOccurs="0"/>
            <xs:element name="CSCNumber" type="base:FourDigitNumberType" minOccurs="0"/>
            <xs:element name="friendlyName" type="base:StringMaxLength70Type" minOccurs="0"/>
            <xs:element name="currencyCode" type="target:CurrencyCodeType" minOccurs="1"/>
            <xs:element name="holdPayment" type="xs:boolean" minOccurs="0"/>
            <xs:element name="paymentStatus" type="xs:string" minOccurs="0"/>
            <xs:element name="processedDate" type="xs:date" minOccurs="0"/>
            <xs:element name="processeDateTime" type="xs:dateTime" minOccurs="0"/>
            <xs:element name="postCode" type="base:StringMaxLength10Type" minOccurs="0"/>
            <xs:element name="countryCode" type="base:StringFixedLength2Type" minOccurs="0"/>
            <xs:element name="productService" type="xs:string" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:simpleType name="PaymentNumberType">
        <xs:restriction base="base:StringMaxLength30Type"/>
    </xs:simpleType>

    <xs:complexType name="PaymentDetailsType">
        <xs:sequence>
            <xs:element name="paymentTypeCode" type="xs:string" minOccurs="1"/>
            <xs:element name="accountNumber" type="base:AccountNumberType" minOccurs="1"/>
            <xs:element name="cardNumber" type="xs:string" minOccurs="1"/>
            <xs:element name="nameOnCard" type="xs:string" minOccurs="0" nillable="true"/>
            <xs:element name="startDateMMYYNumber" type="target:MonthYearType" minOccurs="0"
                nillable="true"/>
            <xs:element name="expiryDateMMYYNumber" type="target:MonthYearType" minOccurs="0"/>
            <xs:element name="amount" type="base:Currency9_2Type" minOccurs="1"/>
            <xs:element name="issueNumber" type="base:TwoDigitNumberType" minOccurs="0"/>
            <xs:element name="AVSHouseStreetNumber" type="xs:string" minOccurs="0"/>
            <xs:element name="CSCNumber" type="xs:string" minOccurs="0"/>
            <xs:element name="currencyCode" type="target:CurrencyCodeType" minOccurs="0"/>
            <xs:element name="holdPayment" type="xs:boolean" minOccurs="0"/>
            <xs:element name="paymentNumber" type="target:PaymentNumberType" minOccurs="1"/>
            <xs:element name="paymentStatus" type="xs:string" minOccurs="0"/>
            <xs:element name="processedDate" type="xs:date" minOccurs="0"/>
             <xs:element name="processedDateTime" type="xs:dateTime" minOccurs="0"/>
            <xs:element name="postCode" type="base:StringMaxLength10Type" minOccurs="0"/>
            <xs:element name="countryCode" type="base:StringFixedLength2Type" minOccurs="0"/>
            <xs:element name="productService" type="xs:string" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="MonthYearType">
        <xs:sequence>
            <xs:element name="month" type="base:MonthOfYearType"/>
            <xs:element name="year" type="base:TwoDigitYearType"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ErrorType">
        <xs:sequence>
            <xs:element name="code" type="xs:string" minOccurs="0"/>
            <xs:element name="description" type="xs:string" minOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:simpleType name="CurrencyCodeType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="GBP"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="ChequeType">
        <xs:sequence>
            <xs:element name="PaymentType" type="base:PaymentTypeType" minOccurs="1"/>
        </xs:sequence>
    </xs:complexType>  

    <xs:complexType name="PaymentCardType">
        <xs:sequence>
            <xs:element name="CardNumber" type="base:CCDCCardNumberType" minOccurs="0"/>
            <xs:element name="NameOnCard" type="base:NameOnCardType" minOccurs="0"/>
            <xs:element name="FriendlyName" type="base:StringMaxLength70Type" minOccurs="0"/>
            <xs:element name="StartYear" type="base:YearNumberType" minOccurs="0"/>
            <xs:element name="StartMonth" type="base:MonthNumberType" minOccurs="0"/>
            <xs:element name="ExpiryYear" type="base:YearNumberType" minOccurs="0"/>
            <xs:element name="ExpiryMonth" type="base:MonthNumberType" minOccurs="0"/>            
            <xs:element name="CreditSecurityCode" type="base:CreditSecurityCodeType" minOccurs="0"/>
            <xs:element name="PostCode" type="base:StringMaxLength10Type" minOccurs="0"/>
            <xs:element name="CountryCode" type="base:StringFixedLength2Type" minOccurs="0"/>
            <xs:element name="HouseStreetNumber" type="xs:string" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="PaymentCardTokenType">
        <xs:sequence>
            <xs:element name="CardReference" type="base:PCI_CardOnFileReferenceType" minOccurs="1"/>
            <xs:element name="CardNumber" type="base:PCI_CCDCCardNumberType" minOccurs="0"/>
            <xs:element name="NameOnCard" type="base:NameOnCardType" minOccurs="0"/>
            <xs:element name="StartYear" type="base:YearNumberType" minOccurs="0"/>
            <xs:element name="StartMonth" type="base:MonthNumberType" minOccurs="0"/>
            <xs:element name="ExpiryYear" type="base:YearNumberType" minOccurs="0"/>
            <xs:element name="ExpiryMonth" type="base:MonthNumberType" minOccurs="0"/>            
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="UpdatePaymentCardType">
        <xs:choice>
            <xs:element name="PaymentCard" type="target:PaymentCardType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="PaymentCardToken" type="target:PaymentCardTokenType" minOccurs="1"/>
            <xs:element name="CancellationInformation" type="target:CancellationPaymentType" minOccurs="1" maxOccurs="1"/>
        </xs:choice>
    </xs:complexType>
    
    <xs:complexType name="UpdateDirectDebitType">
        <xs:choice>
            <xs:element name="DirectDebit" type="target:DirectDebitType" minOccurs="1"/>
            <xs:element name="SEPADirectDebit" type="target:SEPADirectDebitType" minOccurs="1"/>
            <xs:element name="CancellationInformation" type="target:CancellationPaymentType" minOccurs="1"/>
        </xs:choice>
    </xs:complexType>

    <xs:complexType name="PaymentCardQueryType">
        <xs:sequence>
            <xs:element name="PaymentType" type="base:PaymentTypeType" minOccurs="1"/>
            <xs:element name="CardNumber" type="base:CCDCCardNumberType" minOccurs="0"/>
            <xs:element name="NameOnCard" type="base:NameOnCardType" minOccurs="0"/>
            <xs:element name="PaymentCardStatus" type="target:CardPaymentStatusType" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    
    
    <xs:complexType name="DirectDebitResponseBaseType">
        <xs:sequence>
            <xs:element name="BankName" type="base:BankNameType" minOccurs="0"/>
            <xs:element name="NameOfPayer" type="base:NameOfPayerType" minOccurs="0"/>
            <xs:element name="DateMandateReceived" type="xs:date" minOccurs="0"/>
            <xs:element name="DirectDebitPaymentStatus" type="target:DirectDebitPaymentStatusType" minOccurs="0"/>
            <xs:element name="DDSetupMethodCode" type="base:DDSetupMethodCodeType" minOccurs="0"/>
	</xs:sequence>
    </xs:complexType>

    <xs:complexType name="DirectDebitResponseType">
        <xs:complexContent>
            <xs:extension base="target:DirectDebitResponseBaseType">
				<xs:sequence>
		            <xs:element name="PaymentType" type="base:PaymentTypeType" minOccurs="1"/>
		            <xs:element name="BankAccountNumber" type="base:BankAccountNumberType" minOccurs="0"/>
		            <xs:element name="BankSortCode" type="base:BankSortCodeType" minOccurs="0"/>
		            <xs:element name="ControlDigit" type="base:ControlDigitType" minOccurs="0"/>
		            <xs:element name="DDAccountName" type="base:StringMaxLength60Type" minOccurs="0"/>
		            <xs:element name="ConfirmationDate" type="xs:date" minOccurs="0"/>
				</xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    
    <xs:complexType name="SEPADirectDebitResponseType">
        <xs:complexContent>
            <xs:extension base="target:DirectDebitResponseBaseType">
				<xs:sequence>
		            <xs:element name="IBAN" type="base:IBANType" minOccurs="0"/>
					<xs:element name="BIC" type="base:BICType" minOccurs="0"/>
		            <xs:element name="MandateReference" type="base:MandateReferenceType" minOccurs="0"/>
					<xs:element name="MandateSignatureDate" type="xs:date" minOccurs="0"/>
				</xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    
    <xs:complexType name="DirectDebitBaseType">
        <xs:sequence>
            <xs:element name="NameOfPayer" type="base:NameOfPayerType" minOccurs="0"/>
            <xs:element name="DateMandateReceived" type="base:ClearableDateType" minOccurs="0"/>
            <xs:element name="DDSetupMethodCode" type="base:DDSetupMethodCodeType" minOccurs="0"/> 
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="DirectDebitType">
          <xs:complexContent>
            <xs:extension base="target:DirectDebitBaseType">
				<xs:sequence>
		            <xs:element name="DDAccountName" type="base:StringMaxLength60Type" minOccurs="0"/>
		            <xs:element name="BankAccountNumber" type="base:BankAccountNumberType" minOccurs="0"/>
		            <xs:element name="BankSortCode" type="base:BankSortCodeType" minOccurs="0"/>
		            <xs:element name="ControlDigit" type="base:ControlDigitType" minOccurs="0"/>
		            <xs:element name="DirectDebitPaymentStatus" type="target:DirectDebitPaymentStatusType" minOccurs="0"/>
        		</xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    
    <xs:complexType name="SEPADirectDebitType">
        <xs:complexContent>
            <xs:extension base="target:DirectDebitBaseType">
				<xs:sequence>
		            <xs:element name="IBAN" type="base:IBANType" minOccurs="0"/>
					<xs:element name="BIC" type="base:BICType" minOccurs="0"/>
				    <xs:element name="MandateReference" type="base:MandateReferenceType" minOccurs="0"/>
				    <xs:element name="MandateSignatureDate" type="xs:date" minOccurs="0"/>
	        	</xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
     
    <xs:complexType name="CancellationPaymentType">
        <xs:sequence>
            <xs:element name="ReasonCode" type="base:StringMaxLength4Type" minOccurs="1" maxOccurs="1"/>
            <xs:element name="CancellationDate" type="xs:date" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:simpleType name="PaymentTypeDescriptionType">
        <xs:restriction base="base:StringMaxLength30Type"/>
    </xs:simpleType>
    <xs:simpleType name="CardDescriptionType">
        <xs:restriction base="base:StringMaxLength30Type"/>
    </xs:simpleType>
        <xs:simpleType name="ExplanationType">
        <xs:restriction base="base:StringMaxLength40Type"/>
    </xs:simpleType>

    <xs:simpleType name="DeliveryAddressNumberType">
    	<xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="99999999"/>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="CardPaymentStatusType">
        <xs:restriction base="base:StringMaxLength6Type"/>
    </xs:simpleType>

    <xs:simpleType name="DirectDebitPaymentStatusType">
        <xs:restriction base="base:StringMaxLength6Type"/>
    </xs:simpleType>
  </xs:schema>
